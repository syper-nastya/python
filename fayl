import pyautogui
import time
namber = int(input("Введите число "))
namber = namber + 1
n = int(0)
for step in range(1, namber):
    print(step)
pyautogui.hotkey('win', 'd')
time.sleep(1)
for step in range(1, namber):
    n = n + 1
    pyautogui.hotkey('win', 'r')
    time.sleep(1)
    pyautogui.moveTo(190, 692)
    pyautogui.click()
    time.sleep(1)
    pyautogui.hotkey('ctrl', 's')
    pyautogui.moveTo(721, 380)
    pyautogui.click()
    time.sleep(1)
    pyautogui.moveTo(291,490)
    pyautogui.click()
    pyautogui.press('backspace')
    pyautogui.press('backspace')
    time.sleep(1)
    pyautogui.press('backspace')
    pyautogui.press('backspace')
    pyautogui.press('backspace')
    time.sleep(1)
    pyautogui.write(str(n))
    time.sleep(1)
    pyautogui.press('enter')
    pyautogui.press('enter')
    time.sleep(1)
pyautogui.hotkey('win', 'd')
pyautogui.moveTo(343, 247)
pyautogui.doubleClick()
